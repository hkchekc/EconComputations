    REAL(KIND=8), PARAMETER:: AGG_K =(I*AGG_KSTEP, I=1,N_AGG_K) + AGG_K_MIN - AGG_KSTEP

    REAL(KIND=8),PARAMETER:: U_G=0.04, U_B=0.1
